<?xml version="1.0" encoding="UTF-8"?>
<Patch>

  
  <!-- Grimworld 40k -->
  <Operation Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[@Name="GW_DoorBase"]</xpath>
		<value>
		<building>
				<isAirtight>true</isAirtight>
		</building>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[@Name="GW_AutoDoorBase"]</xpath>
		<value>
		<building>
				<isAirtight>true</isAirtight>
		</building>
		</value>
	</Operation>

	<Operation Class="PatchOperationSequence">
	<operations>
	  <li Class="PatchOperationAdd" MayRequire="SmashPhil.VehicleFramework">
        <xpath>Defs/ThingDef[@Name="GW_GarageDoor"]</xpath>
		<value>
		<building>
				<isAirtight>true</isAirtight>
		</building>
		</value>
		</li>
	<li Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[@Name="GW_BlastDoorBase"]</xpath>
		<value>
		<building>
				<isAirtight>true</isAirtight>
		</building>
		</value>
		</li>	
		</operations>
	</Operation>
<!--Aspectus Imperialis Patches-->
	<Operation Class="PatchOperationSequence">
	  <operations>
	  <li Class="PatchOperationAdd" MayRequire="Grimworld.AspectusImperialis">
        <xpath>Defs/ThingDef[defName="GW40k_2x1Gate"]</xpath>
		<value>
		<building>
				<isAirtight>true</isAirtight>
		</building>
		</value>
		</li>
		<li Class="PatchOperationAdd" MayRequire="Grimworld.AspectusImperialis">
        <xpath>Defs/ThingDef[defName="GW40k_3x1Gate"]</xpath>
		<value>
		<building>
				<isAirtight>true</isAirtight>
		</building>
		</value>
		</li>
		</operations>
	</Operation>

</Patch>